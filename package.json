{
  "name": "reptimer",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "prebuild": "node tools/version.js",
    "start": "npm run prebuild && ng serve --port 4201 --open",
    "build": "ng build",
    "deployment": "npm run prebuild && ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n-extract": "node_modules\\.bin\\ngc -p tsconfig.app.json && node_modules\\.bin\\localize-extract -l de-DE -s ./dist/out-locl/**/*.js -f xlf -o src/locale/messages.de-DE.xlf",
    "i18n-createJson": "tsc tools/dev-localizer.ts && node tools/dev-localizer.js",
    "i18n": "npm run i18n-extract && npm run i18n-createJson"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "file-saver": "^2.0.5",
    "ngx-papaparse": "^5.0.0",
    "rxjs": "~6.6.0",
    "ts-md5": "^1.2.7",
    "tslib": "^2.0.0",
    "xliff": "^5.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@angular/localize": "^14.2.12",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  },
  "browser": {
    "crypto": false
  }
